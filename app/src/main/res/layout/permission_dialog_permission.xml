<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>
        <import type="com.ioscharginging.iosanimationation.ui.custom.ScalableCardView.ScaleType"/>
    </data>

    <com.ioscharginging.iosanimationation.ui.custom.ScalableCardView xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="270:142"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        app:cardElevation="0dp"
        app:cardBackgroundColor="@android:color/transparent"
        scaleType="@{ScaleType.BY_WIDTH}"
        scalableCornerRadius="@{0.074074074f}">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#EBF8F8F8">

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintDimensionRatio="238:22"
                app:layout_constraintHeight_percent="0.1549"
                app:layout_constraintVertical_bias="0.158333"
                android:gravity="center"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="17sp"
                app:autoSizeMinTextSize="1sp"
                android:textColor="#0A0A0B"
                android:fontFamily="@font/sf_pro_text_bold"
                android:text="@string/get_permissions"/>

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintDimensionRatio="238:44"
                app:layout_constraintHeight_percent="0.309859"
                app:layout_constraintVertical_bias="0.469"
                android:gravity="center_horizontal"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="13sp"
                app:autoSizeMinTextSize="1sp"
                android:textColor="#990A0A0B"
                android:fontFamily="@font/sf_pro_text_regular"
                android:text="@string/allow_display_over_other_apps"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintDimensionRatio="270:1"
                app:layout_constraintVertical_bias="0.6879432"
                android:background="#260A0A0B"/>

            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintDimensionRatio="1:44"
                app:layout_constraintHeight_percent="0.309859"
                android:background="#260A0A0B"/>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/buttonNo"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintDimensionRatio="134.5:44"
                app:layout_constraintHeight_percent="0.309859">
                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintDimensionRatio="103:22"
                    app:layout_constraintHeight_percent="0.5"
                    android:gravity="center"
                    app:autoSizeTextType="uniform"
                    app:autoSizeMaxTextSize="17sp"
                    app:autoSizeMinTextSize="1sp"
                    android:textColor="#027AFF"
                    android:fontFamily="@font/sf_pro_text_regular"
                    android:text="@string/not_now"/>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/buttonYes"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintDimensionRatio="134.5:44"
                app:layout_constraintHeight_percent="0.309859">
                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintDimensionRatio="103:22"
                    app:layout_constraintHeight_percent="0.5"
                    android:gravity="center"
                    app:autoSizeTextType="uniform"
                    app:autoSizeMaxTextSize="17sp"
                    app:autoSizeMinTextSize="1sp"
                    android:textColor="#027AFF"
                    android:fontFamily="@font/sf_pro_text_regular"
                    android:text="@string/allow"/>
            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.ioscharginging.iosanimationation.ui.custom.ScalableCardView>

</layout>